<ScrollPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gestorestacionamientofx.controllers.HelloController">
   <content>
      <VBox alignment="CENTER" prefHeight="320.0" prefWidth="243.0" spacing="20.0" style="-fx-background-color: grey;">
        <padding>
          <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
      
        <Label fx:id="welcomeText" />
         <Pane />
         <Pane prefHeight="18.0" prefWidth="53.0">
            <children>
               <Label prefHeight="18.0" prefWidth="53.0" text="Label" textOverrun="CLIP" />
            </children>
         </Pane>
        <Button alignment="TOP_RIGHT" contentDisplay="CENTER" onAction="#onHelloButtonClick" prefHeight="17.0" prefWidth="59.0" text="Hello!" textAlignment="CENTER" />
      </VBox>
   </content>
</ScrollPane>
